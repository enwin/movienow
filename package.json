{
  "name": "movienow",
  "version": "0.8.1",
  "description": "display next movie showtime around you",
  "main": "server.js",
  "scripts": {
    "serve": "nodemon",
    "js": "browserify ./script/app.js -o ./www/app.js -t [ babelify --presets [ es2015 ] ]",
    "sw": "browserify ./script/sw.js -o ./www/sw.js",
    "watch:sw": "onchange 'script/sw.js' -- npm run sw",
    "watch:js": "onchange 'script/app.js' -- npm run js",
    "svg": "svgo -f ./_source/svg --pretty --config=svgo.yml",
    "start": "npm run serve"
  },
  "author": "enwin <tifftafftoff@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "connect-browser-sync": "^2.0.1",
    "morgan": "^1.7.0",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "svgo": "^0.7.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.11.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "browser-sync": "^2.17.5",
    "browserify": "^13.1.1",
    "browserify-middleware": "^7.1.0",
    "cheerio": "^0.22.0",
    "chouchenn": "^1.0.2",
    "compression": "^1.6.2",
    "connect-expires": "0.0.2",
    "connect-mongo": "^1.3.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "fastclick": "^1.0.6",
    "geocoder": "^0.2.2",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "js-yaml": "^3.6.1",
    "lodash": "^4.16.6",
    "moment": "^2.15.2",
    "mongoose": "^4.6.6",
    "piwik-tracker": "^1.0.0",
    "request": "^2.78.0",
    "request-promise-native": "^1.0.3",
    "route-parser": "0.0.4",
    "serve-static": "^1.11.1",
    "showtimes": "github:enwin/showtimes",
    "slack-node": "^0.2.0",
    "slug": "^0.9.1",
    "stylus": "^0.54.5"
  }
}
