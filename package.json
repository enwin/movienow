{
  "name": "movienow",
  "version": "0.7.3",
  "description": "display next movie showtime around you",
  "main": "server.js",
  "scripts": {
    "serve": "nodemon",
    "js": "browserify ./script/app.js -o ./www/app.js -t [ babelify --presets [ es2015 ] ]",
    "sw": "browserify ./script/sw.js -o ./www/sw.js",
    "watch:sw": "onchange 'script/sw.js' -- npm run sw",
    "watch:js": "onchange 'script/app.js' -- npm run js",
    "svg": "svgo -f ./_source/svg --pretty --config=svgo.yml",
    "start": "npm run serve"
  },
  "author": "enwin <tifftafftoff@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-polyfill": "^6.6.1",
    "browser-sync": "^2.11.1",
    "connect-browser-sync": "^2.0.1",
    "connect-mongo": "^1.1.0",
    "moment": "^2.11.2",
    "morgan": "^1.7.0",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "request-promise": "^2.0.1",
    "route-parser": "0.0.4",
    "svgo": "^0.6.1",
    "watchify": "^3.7.0",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "browserify-middleware": "^7.0.0",
    "cheerio": "^0.20.0",
    "chouchenn": "^1.0.1",
    "compression": "^1.6.1",
    "connect-expires": "0.0.2",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "fastclick": "^1.0.6",
    "geocoder": "^0.2.2",
    "jade": "^1.11.0",
    "jadeify": "^4.6.0",
    "js-yaml": "^3.5.3",
    "lodash": "^4.5.0",
    "mongoose": "^4.4.6",
    "piwik-tracker": "^0.1.2",
    "serve-static": "^1.10.2",
    "showtimes": "enwin/showtimes",
    "slack-node": "^0.2.0",
    "slug": "^0.9.1",
    "stylus": "^0.53.0"
  }
}
